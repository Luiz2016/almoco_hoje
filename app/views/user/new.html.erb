<%= simple_form_for @user do |f| %>
 <%= f.input :name, label: 'Nome' %>
 <%= f.input :email, label: 'E-mail' %>
 <%= f.input :interests, label: 'Interesses' %>
 <%= f.input :company, label: 'Empresa' %>
 <%= f.input :address, label: 'Região', collection:['Paulista', 'Berrini',
                                                    'Faria Lima', 'Centro',
                                                    'Pinheiros'] %>
 <%= f.input :date, label: 'Data', html5: true, as: :date %>
 <%= f.input :time, label: 'Horário', html5: true %>
 <%= f.submit 'Quero receber convites!' %>
<% end %>
